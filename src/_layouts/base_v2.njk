---
templateEngineOverride: njk,md
---
<!DOCTYPE html>
<html lang="en">
  <head>
    {% include '../_includes/head/head_v1.njk' %}
    </head>
  <!-- layout: {{layout}} -->
  <body class="text">
    <header>
      <div  class="navbar navbar-expand-md navbar-dark bd-navbar bg-blue">{% include '../_includes/header.njk' %}</div>{% if page.fileSlug != "" %}<nav aria-label="breadcrumb" class="container">{% if '/outreach/*' in page.url %}{% include '../_includes/breadcrumbs/outreach-bc.njk' %}{% else %}{% include '../_includes/breadcrumbs/base-bc.njk' %}{% endif %}</nav>{% endif %}
    </header>
    <div class="{{ 'landing' if page.fileSlug == ''}}">
      <main class="d-flex container flex-column">{{ content | safe }}</main>
    </div>
    <footer class="bg-blue">{% include '../_includes/footer.html' %}</footer>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="{{site.url + '/js/custom.js'}}" defer></script>
  </body>
</html>