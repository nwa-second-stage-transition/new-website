---
templateEngineOverride: njk,md
---
<!DOCTYPE html>
<html lang="en">
  <head>
    {% include '../_includes/head.html' %}
    <title>{{ title }}</title>{% if page.fileSlug == ''%}<link rel="preload" href="{{site.url + '/images/trees-along-path-2300.webp'}}" as="image">{% endif %}{% if page.fileSlug == '/admin/'%}<script defer src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>{% endif %}
    </head>
  <body class="text">
    <header>
      <div  class="navbar navbar-expand-md navbar-dark bd-navbar bg-blue">{% include '../_includes/header.njk' %}</div>{% if page.fileSlug != "" %}<nav aria-label="breadcrumb" class="container">{% if '/outreach/*' in page.url %}{% include '../_includes/breadcrumbs/outreach-bc.njk' %}{% else %}{% include '../_includes/breadcrumbs/base-bc.njk' %}{% endif %}</nav>{% endif %}
    </header>
    <div class="{{ 'landing' if page.fileSlug == ''}}">
      <main class="d-flex container flex-column">{{ content | safe }}</main>
    </div>
    <footer class="bg-blue">{% include '../_includes/footer.html' %}</footer>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="{{site.url + '/javascript/footer-javascript.js'}}"></script>
  </body>
</html>